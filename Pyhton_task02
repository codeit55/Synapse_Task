def earliest_lumos(seq):
    seq = seq.lower()  
    
    l = u = m = o = s = 0  
    
    for i in range(len(seq)):
        ch = seq[i]
        if ch == 'l': l = 1
        if ch == 'u': u = 1
        if ch == 'm': m = 1
        if ch == 'o': o = 1
        if ch == 's': s = 1
        
        if l and u and m and o and s:
            return i + 1   
    
    return -1
